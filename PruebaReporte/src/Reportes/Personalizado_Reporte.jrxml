	<queryString language="SQL">
		<![CDATA[SELECT medicamentos.nombre AS nombre_medicamento,
	bodegas.nombre AS nombre_bodega,
	medicamentos.fecha_vencimiento,
	tipo_medicamento.nombre AS nombre_tipo_medicamento
FROM medicamentos
	INNER JOIN bodegas ON 
	 medicamentos.id_bodega = bodegas.id_bodega 
	INNER JOIN tipo_medicamento ON 
	 medicamentos.id_tipo_med = tipo_medicamento.id_tipo_med
	 WHERE bodegas.nombre  =  $P{Filtro_bodega} AND tipo_medicamento.nombre =  $P{Filtro_tipo_medicamento} ]]>
	</queryString>
